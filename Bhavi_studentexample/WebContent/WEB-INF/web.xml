<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Bhavi_studentexample</display-name>
  <welcome-file-list>
    <welcome-file>student.html</welcome-file>
  </welcome-file-list>
  <!-- Configure StudentServlet -->
  <servlet>
    <servlet-name>abc</servlet-name>
    <servlet-class>com.servlet.controller.StudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/first</url-pattern>
  </servlet-mapping>
  <!-- Configure RetriveServlet -->
  <servlet>
  <servlet-name>Bhavisha</servlet-name>
  <servlet-class>com.servlet.controller.RetriveServlet</servlet-class>
  
  </servlet>
  <servlet-mapping>
  <servlet-name>Bhavisha</servlet-name>
  <url-pattern>/second</url-pattern>
  
  </servlet-mapping>
</web-app>